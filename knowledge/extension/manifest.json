{
  "manifest_version": 2,

  "name": "Vidiew",
  "description": "This extension helps customer to find multi-media reviews when shopping online.",
  "version": "1.0",

  "icons": {
    "128": "icon.png"
  },

  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "page": "preview.html"
  },

  "content_scripts": [
    {
      "matches": ["http://www.walmart.com/*", "https://www.walmart.com/*"],
      "js": ["jquery-1.11.1.min.js", "vidiew.js"],
      "css": ["vidiew.css"],
      "run_at": "document_start"
    }
  ],

  "web_accessible_resources": [
    "*.png",
    "*.html",
    "*.css",
    "*.js"
  ]
}